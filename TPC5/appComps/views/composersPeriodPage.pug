extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1 Composers from #{periodo}
        .w3-container
            table.w3-table-all
                thead
                    tr
                        th Id
                        th Name
                        th Period
                tbody
                    each composer in lComp || []
                        tr
                            td= composer.id
                            td= composer.nome
                            td= composer.periodo
        footer.w3-container.w3-teal
            address
                a(href="/periodos") Voltar
